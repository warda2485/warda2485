#include <stdio.h>
int main (){
 char matrix [5][5]= {
  {'1','2','3','4','5'},
  {'7','a','c','8','d'},
  {'c','9','4','z','8'},
  {'5','6','p','n','3'},
  {'2','9','t','m','k'},
 };
 int i,j;
 printf(" full matrix :\n ");
 for(i=0;i<5;i++){
   for(j=0;j<5;j++){
    printf("%c" ,matrix[i][j]);
   }
   printf("\n");
 }
 printf("/n rews with even indices:\n");
  for(i=0;i<5;i+=2){
   for(j=0;j<5;j++){
    printf("%c" ,matrix[i][j]);
   }
   printf("\n");
 }
 printf("\n 0dd indexed elements in each row :\n");
  for(i=0;i<5;i++){
   for(j=0;j<5;j+=2){
    printf("%c" ,matrix[i][j]);
   }
   printf("\n");
 }
 return 0; 
}
